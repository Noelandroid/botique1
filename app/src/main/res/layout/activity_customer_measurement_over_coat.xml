<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    tools:context=".CustomerMeasurementOverCoatActivity"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/toolbar_height"
            android:background="@color/colorPrimary"
            android:gravity="center">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="visible">

                <TextView
                    android:id="@+id/toolbar_title"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="-62dp"
                    android:gravity="center"
                    android:text="username"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    android:textStyle="bold" />

            </FrameLayout>

        </android.support.v7.widget.Toolbar>


        <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/pageMarginHorizontal"
            android:paddingTop="@dimen/pageMarginVertical"
            android:paddingEnd="@dimen/pageMarginHorizontal"
            android:paddingBottom="@dimen/pageMarginVertical">

            <TextView
                android:id="@+id/tvLabelTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"
                android:text="@string/customer_measurement"
                android:textColor="@color/colorPrimary"
                android:textSize="18sp"
                android:textStyle="normal"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <android.support.constraint.Guideline
                android:id="@+id/guide_line_20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.20" />


            <TextView
                android:id="@+id/tvLabelFitting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/fitting"
                app:layout_constraintBaseline_toBaselineOf="@id/checkLooseFit"
                app:layout_constraintRight_toLeftOf="@+id/checkLooseFit"
                app:layout_constraintStart_toStartOf="parent" />

            <android.support.v7.widget.AppCompatCheckBox
                android:id="@+id/checkLooseFit"
                android:layout_width="0dp"

                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="@string/loose_fit"
                app:layout_constraintLeft_toLeftOf="@id/guide_line_20"
                app:layout_constraintRight_toLeftOf="@id/checkMedium"
                app:layout_constraintTop_toBottomOf="@id/tvLabelTitle" />

            <android.support.v7.widget.AppCompatCheckBox
                android:id="@+id/checkMedium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/medium"
                app:layout_constraintBaseline_toBaselineOf="@id/checkLooseFit"
                app:layout_constraintLeft_toRightOf="@id/checkLooseFit"
                app:layout_constraintRight_toLeftOf="@id/checkTightFit" />

            <android.support.v7.widget.AppCompatCheckBox
                android:id="@+id/checkTightFit"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tight_fit"
                app:layout_constraintBaseline_toBaselineOf="@id/checkLooseFit"
                app:layout_constraintLeft_toRightOf="@id/checkMedium"
                app:layout_constraintRight_toRightOf="parent" />


            <TextView
                android:id="@+id/tvLabelTop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/marginTopEdtv"
                android:padding="2dp"
                android:text="@string/overcoat"
                android:textColor="#000"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/checkLooseFit" />


            <TextView
                android:id="@+id/tv_label_length"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="@string/length"
                app:layout_constraintBaseline_toBaselineOf="@id/etShort"
                app:layout_constraintRight_toLeftOf="@id/tv_label_short"
                app:layout_constraintStart_toStartOf="parent" />


            <TextView
                android:id="@+id/tv_label_short"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="@string/short1"
                app:layout_constraintBaseline_toBaselineOf="@id/etShort"
                app:layout_constraintLeft_toLeftOf="@id/guide_line_20"
                app:layout_constraintRight_toLeftOf="@id/etShort" />


            <EditText
                android:id="@+id/etShort"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/marginTopEdtv"
                android:layout_marginEnd="@dimen/marginEndEdtvBeforeTv"
                android:background="@drawable/edit_text_background"
                android:maxLines="1"
                app:layout_constraintLeft_toRightOf="@id/tv_label_short"
                app:layout_constraintRight_toLeftOf="@id/tv_label_long"
                app:layout_constraintTop_toBottomOf="@id/tvLabelTop" />


            <TextView
                android:id="@+id/tv_label_long"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="@string/long1"
                app:layout_constraintBaseline_toBaselineOf="@id/etShort"
                app:layout_constraintLeft_toRightOf="@id/etShort"
                app:layout_constraintRight_toLeftOf="@id/etLong" />

            <EditText
                android:id="@+id/etLong"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/marginEndEdtvDefault"
                android:background="@drawable/edit_text_background"
                android:maxLines="1"
                app:layout_constraintBaseline_toBaselineOf="@id/etShort"
                app:layout_constraintLeft_toRightOf="@id/tv_label_long"
                app:layout_constraintRight_toRightOf="parent"


                />

            <TextView
                android:id="@+id/tv_label_shoulder"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="@string/shoulder"
                app:layout_constraintBaseline_toBaselineOf="@id/etShoulder"
                app:layout_constraintRight_toLeftOf="@id/tv_label_short"
                app:layout_constraintStart_toStartOf="parent" />



            <EditText
                android:id="@+id/etShoulder"
