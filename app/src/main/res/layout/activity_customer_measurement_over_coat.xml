                android:layout_marginEnd="@dimen/marginEndEdtvDefault"
                android:background="@drawable/edit_text_background"
                android:maxLines="1"
                app:layout_constraintBaseline_toBaselineOf="@id/etShoulder"
                app:layout_constraintLeft_toRightOf="@id/tv_label_shoulder_f"
                app:layout_constraintRight_toLeftOf="@id/etF2" />

            <EditText
                android:id="@+id/etF2"
