                app:layout_constraintBaseline_toBaselineOf="@id/etNeckF"
                app:layout_constraintLeft_toRightOf="@id/etNeckF"
                app:layout_constraintRight_toLeftOf="@id/etNeckB" />


            <EditText
                android:id="@+id/etNeckB"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/marginTopEdtv"
                android:layout_marginEnd="@dimen/marginEndEdtvBeforeTv"
                android:background="@drawable/edit_text_background"
                android:maxLines="1"
                app:layout_constraintBaseline_toBaselineOf="@id/etNeckF"
                app:layout_constraintLeft_toRightOf="@id/tv_label_neck_b"
                app:layout_constraintRight_toLeftOf="@id/tv_label_neck_w" />

            <TextView
                android:id="@+id/tv_label_neck_w"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="@string/w"
                app:layout_constraintBaseline_toBaselineOf="@id/etNeckF"
                app:layout_constraintLeft_toRightOf="@id/etNeckB"
                app:layout_constraintRight_toLeftOf="@id/etNeckW" />


            <EditText
                android:id="@+id/etNeckW"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/marginEndEdtvBeforeTv"
                android:background="@drawable/edit_text_background"
                android:maxLines="1"
                app:layout_constraintBaseline_toBaselineOf="@id/etNeckF"
                app:layout_constraintLeft_toRightOf="@id/tv_label_neck_w"
                app:layout_constraintRight_toLeftOf="@id/tv_label_neck_square" />


            <TextView
                android:id="@+id/tv_label_neck_square"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="@string/square"
                app:layout_constraintBaseline_toBaselineOf="@id/etNeckF"
                app:layout_constraintLeft_toRightOf="@id/etNeckW"
                app:layout_constraintRight_toLeftOf="@id/etNeckSquare" />


            <EditText
                android:id="@+id/etNeckSquare"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/edit_text_background"
                android:maxLines="1"
                app:layout_constraintBaseline_toBaselineOf="@id/etNeckF"
                app:layout_constraintLeft_toRightOf="@id/tv_label_neck_square"
                app:layout_constraintRight_toRightOf="parent" />






            <TextView
                android:id="@+id/tv_label_remarks"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="@string/remarks"
                app:layout_constraintBaseline_toBaselineOf="@id/etRemarks"
                app:layout_constraintRight_toLeftOf="@id/etRemarks"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/etRemarks"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/marginTopEdtv"
                android:layout_marginEnd="@dimen/marginEndEdtvDefault"
                android:background="@drawable/edit_text_background"
                android:minHeight="90dp"
                app:layout_constraintLeft_toLeftOf="@id/guide_line_20"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/etNeckF" />



            <android.support.constraint.Guideline
                android:id="@+id/guide_line_25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.25" />


            <android.support.constraint.Guideline
                android:id="@+id/guide_line_75"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.75" />

            <ru.katso.livebutton.LiveButton
                android:id="@+id/btnSave"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="45dp"
                android:paddingTop="@dimen/double_default_margin"
                android:paddingBottom="@dimen/double_default_margin"
                android:text="@string/save"
                android:textAllCaps="true"
                android:textColor="@color/white"
                app:backgroundColor="@color/green"
                app:corners="@dimen/button_corner"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="@id/guide_line_25"
                app:layout_constraintRight_toRightOf="@id/guide_line_75"
                app:layout_constraintTop_toBottomOf="@id/etRemarks"
                app:normalHeight="@dimen/button_normal_height"
                app:pressedHeight="@dimen/button_pressed_height"
                app:shadowColor="@color/darkGreen" />



        </android.support.constraint.ConstraintLayout>


    </LinearLayout>
</ScrollView>