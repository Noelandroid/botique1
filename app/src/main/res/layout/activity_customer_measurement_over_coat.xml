                android:layout_marginEnd="@dimen/marginEndEdtvBeforeTv"
                android:background="@drawable/edit_text_background"
                android:maxLines="1"
                app:layout_constraintLeft_toLeftOf="@id/guide_line_20"
                app:layout_constraintRight_toLeftOf="@id/tv_label_shoulder_f"
                app:layout_constraintTop_toBottomOf="@id/etShort" />

            <TextView
                android:id="@+id/tv_label_shoulder_f"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
